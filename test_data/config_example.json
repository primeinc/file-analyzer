{
  "name": "Test Configuration",
  "version": 1.0,
  "enabled": true,
  "settings": {
    "scan_timeout": 300,
    "max_files": 1000,
    "extensions": [".txt", ".jpg", ".png", ".pdf"]
  },
  "filters": {
    "include": ["**/documents/**", "**/*.txt"],
    "exclude": ["**/.git/**", "**/temp/**"]
  }
}